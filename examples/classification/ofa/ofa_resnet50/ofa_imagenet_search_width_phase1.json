{
    "dataloader": {
        "imagenet": {
            "train_portion": 0.99,
            "augment_valid": false,
            "colortwist": true,
            "train_path": "/disk1/data/ILSVRC-2012/img_train/",
            "train_file": "/disk1/data/ILSVRC-2012/train.txt",
            "valid_path": "/disk1/data/ILSVRC-2012/img_val/",
            "valid_file": "/disk1/data/ILSVRC-2012/val.txt"
        }
    },
    "network": {
        "ofa_resnet50": {
            "num_classes": 1000,
            "bn_param": [0.9, 1e-5],
            "drop_rate": 0.1,
            "depth_list": [0, 1, 2],
            "expand_ratio_list": [0.2, 0.25, 0.35],
            "width_mult_list": [0.8, 1.0],
            "weights": "log/classification/ofa/ofa_resnet50/imagenet/search/D012_E0.20.250.35_W1.0/weights.h5"
        }
    },
    "optimizer": {
        "train": {
            "weight_decay": 3e-5,
            "lr_scheduler": "CosineSchedulerWarmup",
            "name": "Nesterov",
            "lr": 0.02,
            "momentum": 0.9
            },
        "train_no_decay": {
            "weight_decay": 0,
            "lr_scheduler": "CosineSchedulerWarmup",
            "name": "Nesterov",
            "lr": 0.02,
            "momentum": 0.9
            }
    },
    "hparams": {
        "task": "width",
        "train_image_size_list":[128, 160, 192, 224], 
        "valid_image_size_list": [160, 224],
        "input_shapes": [
             [3, 224, 224]
        ],
        "target_shapes": [
            [1]
        ],
        "no_decay_keys": ["_b$", "/bn/_[^/]*$"],
        "valid_ce_loss": true,
        "valid_genotypes": [
            [0, 0.2, 0.8],
            [2, 0.2, 0.8],
            [0, 0.35, 0.8],
            [2, 0.35, 0.8],
            [0, 0.2, 1.0],
            [2, 0.2, 1.0],
            [0, 0.35, 1.0],
            [2, 0.35, 1.0]
        ],
        "lambda_kd": 1,
        "teacher_network": {
            "ofa_resnet50": {
                "num_classes": 1000,
                "bn_param": [0.9, 1e-5],
                "drop_rate": 0,
                "depth_list": [2],
                "expand_ratio_list": [0.35],
                "width_mult_list": [1.0],
                "weights": "log/classification/ofa/ofa_resnet50/imagenet/search/D2_E0.35_W1.0/weights.h5"
            }
        },
        "epoch": 25,
        "cosine_warmup_epoch": 0,
        "warmup_lr": 0.0025,
        "batch_size_train": 256,
        "batch_size_valid": 200,
        "mini_batch_train": 32,
        "mini_batch_valid": 200,
        "print_frequency": 100,
        "validation_frequency": 1
    }
}
