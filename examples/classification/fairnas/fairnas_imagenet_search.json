{
    "dataloader": {
        "imagenet": {
            "train_portion": 0.96,
            "train_path": "/disk1/data/ILSVRC-2012/img_train/",
            "train_file": "/disk1/data/ILSVRC-2012/train.txt",
            "valid_path": "/disk1/data/ILSVRC-2012/img_val",
            "valid_file": "/disk1/data/ILSVRC-2012/val.txt"
        }
    },
    "network": {
        "fairnas": {
            "num_classes": 1000,
            "settings": [
                [24, 4, 2],
                [40, 4, 2],
                [80, 4, 2],
                [96, 4, 1],
                [192, 4, 2],
                [320, 1, 1]
            ],
            "candidates": [
                "MB6 3x3",
                "MB6 5x5",
                "MB6 7x7"
            ]
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "name": "Momentum",
            "lr_scheduler": "CosineScheduler",
            "lr": 0.1
        }
    },
    "hparams": {
        "epoch": 150,
        "input_shapes": [
            [3, 224, 224]
        ],
        "target_shapes": [
            [1]
        ],
        "batch_size_train": 64,
        "batch_size_valid": 128,
        "mini_batch_train": 32,
        "mini_batch_valid": 128,
        "num_sampled_iter": 6,
        "num_search_samples": 10000,
        "print_frequency": 100
    }
}
