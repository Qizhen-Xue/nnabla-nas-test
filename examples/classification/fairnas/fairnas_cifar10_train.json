{
    "dataloader": {
        "cifar10": {}
    },
    "network": {
        "fairnas": {
            "num_classes": 10,
            "settings": [
                [24, 4, 1],
                [32, 4, 1],
                [64, 4, 2],
                [96, 4, 1],
                [160, 4, 2],
                [320, 1, 1]
            ],
            "genotype": [2, 0, 1, 2, 2, 2, 0, 0, 4, 2, 5, 1, 3, 0, 1, 5, 3, 4, 5, 1, 4]
