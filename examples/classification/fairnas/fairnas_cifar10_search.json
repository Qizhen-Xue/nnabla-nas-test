{
    "dataloader": {
        "cifar10": {
            "train_portion": 0.9
        }
    },
    "network": {
        "fairnas": {
            "num_classes": 10,
            "settings": [
                [24, 4, 1],
                [32, 4, 1],
                [64, 4, 2],
                [96, 4, 1],
                [160, 4, 2],
                [320, 1, 1]
            ]
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "name": "Momentum",
            "lr_scheduler": "CosineScheduler",
            "lr": 0.05
        }
    },
    "hparams": {
        "epoch": 1000,
        "input_shapes": [
            [3, 32, 32]
        ],
        "target_shapes": [
            [1]
        ],
        "batch_size_train": 128,
        "batch_size_valid": 256,
        "mini_batch_train": 128,
        "mini_batch_valid": 256,
        "num_sampled_iter": 6,
        "num_search_samples": 100000,
        "print_frequency": 10
    }
}
