{
    "dataloader": {
        "imagenet": {
            "train_path": "/disk1/data/ILSVRC-2012/img_train/",
            "train_file": "/disk1/data/ILSVRC-2012/train.txt",
            "valid_path": "/disk1/data/ILSVRC-2012/img_val",
            "valid_file": "/disk1/data/ILSVRC-2012/val.txt"
        }
    },
    "network": {
        "fairnas": {
        "num_classes": 1000,
        "settings": [
                [24, 4, 2],
                [40, 4, 2],
                [80, 4, 2],
                [96, 4, 1],
                [192, 4, 2],
                [320, 1, 1]
            ],
        "candidates": [
            "MB6 3x3",
            "MB6 5x5",
            "MB6 7x7"
        ],
        "genotype": [2, 2, 3, 1, 0, 0, 2, 2, 1, 1, 3, 2, 2, 2, 0, 3, 2, 0, 0, 2, 2]
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
                "lr_scheduler": "CosineScheduler",
                "name": "Momentum",
                "lr": 0.05
            }
    },
    "hparams": {
        "input_shapes": [
            [3, 224, 224]
        ],
        "target_shapes": [
            [1]
        ],
        "epoch": 120,
        "batch_size_train": 64,
        "batch_size_valid": 64,
        "mini_batch_train": 32,
        "mini_batch_valid": 32,
        "print_frequency": 100        
    }
}
