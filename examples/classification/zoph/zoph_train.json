{
    "dataloader": {
        "cifar10": {}
    },
    "network": {
        "zoph": {
            "name": "zoph",
            "n_classes": 10,
            "stem_channels": 128,
            "merging_mode": "concat",
            "random_init": false,
            "param_path": "log/zoph/search/arch.h5"
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "lr_scheduler": "CosineScheduler",
            "name": "Momentum",
            "lr": 0.025
        }
    },
    "hparams": {
        "epoch": 600,
        "batch_size_train": 64,
        "batch_size_valid": 20,
        "mini_batch_train": 64,
        "mini_batch_valid": 20,
        "print_frequency": 50,
        "input_shapes": [
            [3, 32, 32]
        ],
        "target_shapes": [
            [1]
        ]
    }
}