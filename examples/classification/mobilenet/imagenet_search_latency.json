{
    "dataloader": {
        "imagenet": {
            "train_portion": 0.96,
            "train_path": "/disk1/data/ILSVRC-2012/img_train/",
            "train_file": "/disk1/data/ILSVRC-2012/train.txt",
            "valid_path": "/disk1/data/ILSVRC-2012/img_val",
            "valid_file": "/disk1/data/ILSVRC-2012/val.txt"
        }
    },
    "network": {
        "mobilenet": {
            "num_classes": 1000,
            "settings": [
                [24, 4, 2],
                [40, 4, 2],
                [80, 4, 2],
                [96, 4, 1],
                [192, 4, 2],
                [320, 1, 1]
            ],
            "mode": "sample"
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "lr_scheduler": "CosineScheduler",
            "name": "Momentum",
            "lr": 0.025
        },
        "valid": {
            "grad_clip": 5.0,
            "name": "Adam",
            "alpha": 0.001,
            "beta1": 0.0,
            "beta2": 0.999
        },
        "warmup": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "lr_scheduler": "CosineScheduler",
            "name": "Momentum",
            "lr": 0.05
        }
    },
    "regularizer": {
        "LatencyEstimator": {
            "weight": 0.1,
            "bound": 1.0
        }
    },
    "hparams": {
        "epoch": 120,
        "batch_size_train": 64,
        "batch_size_valid": 64,
        "mini_batch_train": 32,
        "mini_batch_valid": 32,
        "warmup": 50,
        "print_frequency": 100,
        "input_shapes": [
            [3, 224, 224]
        ],
        "target_shapes": [
            [1]
        ]
    }
}