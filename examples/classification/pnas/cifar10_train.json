{
    "dataloader": {
        "cifar10": {}
    },
    "network": {
        "pnas": {
            "in_channels": 3,
            "init_channels": 36,
            "num_cells": 15,
            "num_classes": 10,
            "num_choices": 4,
            "auxiliary": true,
            "drop_path": 0.1,
            "genotype": "log/classification/pnas/cifar10/search/arch.h5"
        }
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "lr_scheduler": "CosineScheduler",
            "weight_decay": 3e-4,
            "name": "Momentum",
            "lr": 0.025
        }
    },
    "hparams": {
        "epoch": 600,
        "batch_size_train": 256,
        "batch_size_valid": 100,
        "mini_batch_train": 32,
        "mini_batch_valid": 50,
        "warmup": 0,
        "print_frequency": 20,
        "input_shapes": [
            [3, 32, 32]
        ],
        "target_shapes": [
            [1]
        ]
    }
