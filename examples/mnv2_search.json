{
    "dataset": "cifar10",
    "batch_size_train": 128,
    "batch_size_valid": 64,
    "mini_batch_train": 64,
    "mini_batch_valid": 64,
    "warmup": 0,
    "print_frequency": 20,
    "train_portion": 0.5,
    "output_path": "log/mnv2/search",
    "network": {
        "search_space": "mobilenetv2",
        "name": "mnv2",
	"first_maps": 32,
        "last_maps": 1280,
        "n_classes": 10
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.1
            }
        },
        "valid": {
            "grad_clip": 5.0,
            "solver": {
                "name": "Adam",
                "alpha": 3e-4,
                "beta1": 0.5,
                "beta2": 0.999
            }
        }
    }
}
