{
    "dataset": "cifar10",
    "epoch": 600,
    "batch_size_train": 256,
    "batch_size_valid": 100,
    "mini_batch_train": 64,
    "mini_batch_valid": 100,
    "cutout": 16,
    "warmup": 0,
    "aux_weight": 0.4,
    "print_frequency": 20,
    "network": {
        "search_space": "pnas",
        "in_channels": 3,
        "init_channels": 36,
        "num_cells": 15,
        "num_classes": 10,
        "num_choices": 4,
        "auxiliary": true,
        "drop_path": 0.1,
        "genotype": "log/pnas/search/arch.h5"
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        }
    }
}