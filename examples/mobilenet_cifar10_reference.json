{
    "dataset": "cifar10",
    "input_shape": [3, 32, 32],
    "epoch": 600,
    "batch_size_train": 256,
    "batch_size_valid": 200,
    "mini_batch_train": 256,
    "mini_batch_valid": 200,
    "print_frequency": 25,
    "cutout": 16,
    "network": {
        "search_space": "mobilenet",
        "num_classes": 32,
        "settings": [
            [24, 2, 1],
            [32, 3, 1],
            [64, 4, 2],
            [96, 3, 1],
            [160, 3, 2],
            [320, 1, 1]
        ],
        "candidates": [
            "InvertedResidual_t6_k3"
        ],
        "mode": "sample",
        "skip_connect": false
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "solver": {
                "name": "Momentum",
                "lr": 0.1
            }
        }
    }
}