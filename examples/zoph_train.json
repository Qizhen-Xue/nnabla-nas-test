{
    "dataset": "cifar10",
    "epoch": 600,
    "batch_size_train": 64,
    "batch_size_valid": 20,
    "mini_batch_train": 64,
    "mini_batch_valid": 20,
    "warmup": 0,
    "cutout": 16,
    "print_frequency": 50,
    "network": {
        "search_space": "zoph",
        "name": "zoph", 
        "n_classes": 10,
        "stem_channels": 128,
        "param_path": "log/zoph/search/arch.h5"
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        }
    }
}
