{
    "search": true,
    "dataset": "cifar10",
    "epoch": 150,
    "batch_train": 256,
    "batch_valid": 256,
    "mini_batch_train": 64,
    "mini_batch_valid": 64,
    "warmup": 0,
    "output_path": "log/pnas-con/search",
    "print_frequency": 20,
    "train_portion": 0.9,
    "network": {
        "name": "pnas",
        "in_channels": 3,
        "init_channels": 36,
        "num_cells": 15,
        "num_classes": 10,
        "num_choices": 4,
        "mode": "sample"
    },
    "regularizer": {},
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        },
        "valid": {
            "grad_clip": 5.0,
            "solver": {
                "name": "Adam",
                "alpha": 0.01,
                "beta1": 0.0,
                "beta2": 0.999
            }
        },
        "warmup": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        }
    }
}