{
    "dataset": "cifar10",
    "epoch": 50,
    "batch_size_train": 64,
    "batch_size_valid": 64,
    "mini_batch_train": 16,
    "mini_batch_valid": 16,
    "warmup": 0,
    "print_frequency": 20,
    "train_portion": 0.5,
    "network": {
        "name": "darts",
        "in_channels": 3,
        "init_channels": 16,
        "num_cells": 8,
        "num_classes": 10,
        "shared": true,
        "mode": "full"
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 3e-4,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        },
        "valid": {
            "grad_clip": 5.0,
            "solver": {
                "name": "Adam",
                "alpha": 0.01,
                "beta1": 0.5,
                "beta2": 0.999
            }
        }
    }
}