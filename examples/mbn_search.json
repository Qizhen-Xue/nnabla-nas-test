{
    "dataset": "cifar10",
    "epoch": 200,
    "batch_size_train": 256,
    "batch_size_valid": 256,
    "mini_batch_train": 256,
    "mini_batch_valid": 256,
    "warmup": 100,
    "cutout": 16,
    "print_frequency": 50,
    "train_portion": 0.9,
    "network": {
        "search_space": "mbn",
        "num_classes": 10,
        "mode": "sample"
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "solver": {
                "name": "Momentum",
                "lr": 0.1
            }
        },
        "valid": {
            "grad_clip": 5.0,
            "solver": {
                "name": "Adam",
                "alpha": 0.01,
                "beta1": 0.5,
                "beta2": 0.999
            }
        },
        "warmup": {
            "grad_clip": 5.0,
            "weight_decay": 4e-5,
            "solver": {
                "name": "Momentum",
                "lr": 0.025
            }
        }
    }
}