{
    "dataset": "cifar10",
    "epoch": 350,
    "batch_size_train": 64,
    "batch_size_valid": 40,
    "mini_batch_train": 64,
    "mini_batch_valid": 40,
    "warmup": 0,
    "cutout": 16,
    "print_frequency": 50,
    "network": {
        "search_space": "mobilenetv2",
        "name": "mnv2",
        "first_maps": 32,
        "last_maps": 1280,
        "n_classes": 10,
        "genotype": "log/mnv2/search/arch.h5"
    },
    "optimizer": {
        "train": {
            "grad_clip": 5.0,
            "weight_decay":4e-5,
            "solver": {
                "name": "Momentum",
                "lr": 0.1
            }
        }
    }
}
